<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="true">
    <shape>
        <solid
            android:color="#90AEE162"/>
        <stroke
            android:width="1dp"
            android:color="#AEE162" />
        <corners
            android:radius="10dp" />
        <padding
            android:left="10dp"
            android:top="10dp"
            android:right="10dp"
            android:bottom="10dp" />
    </shape>
</item>
<item>
    <shape>
        <gradient
            android:startColor="#90AEE162"
            android:endColor="#FF4D8419"
            android:angle="270"/>
        <stroke
            android:width="1dp"
            android:color="#4D8419"/>
        <corners
            android:radius="10dp"/>
        <padding
            android:left="10dp"
            android:top="10dp"
            android:right="10dp"
            android:bottom="10dp"/>
    </shape>
</item>
</selector>